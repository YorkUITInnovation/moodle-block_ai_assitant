<div class="folder">
    
    {{#sections}}
        <input type="checkbox" id="{{idname}}">
        <label for="{{idname}}"><i class="fa fa-folder"></i>  {{name}}</label>
        <div class="content">
            {{#modules}}
                <p><input 
                type="checkbox"
                class="courseModuleCheckbox"
                data-filename="{{#content}}{{file_name}}{{/content}}" 
                data-content="{{#content}}{{content}}{{/content}}"
        
                >{{name}}</p>
            {{/modules}}
        </div>
    {{/sections}}
</div>